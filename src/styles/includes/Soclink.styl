.Soclink
  display inline-block
  width 32px
  height 32px
  border 1px solid $darkBrown
  border-radius 50%
  background-position center center
  background-size 12px auto
  background-repeat no-repeat
  @extend .transition
  
  // @media $xlMinus
  //   width 23px
  //   height 23px
  //   background-size 9px
  
  &:hover
    background-color $darkBrown
  
  &-white
    border-color $white
    
    &:hover
      background-color $white
  
