.HousePlan
  position relative
  width 90%
  max-width 557px

  @media $smMinus
    margin 0 auto

  &_img
    display block
    width 100%

  &_scheme
    position absolute
    top 0
    left 0
    width 100%
    height 100%

  &_item
    fill rgb(68, 55, 50)
    opacity 0
    cursor pointer

    &:hover
      opacity 0.2

