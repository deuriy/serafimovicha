.MapIcons
	@media $smMinus
		overflow auto

	&_list
		list-style none
		display grid
		grid-template-columns repeat(auto-fit, minmax(100px, auto))
		gap 20px 20px
		padding 40px 0 46px 40px
		border-bottom 1px solid $lightGrey
		
		@media $xlMinus
			padding 30px
		
		@media $smMinus
			width 768px
			padding 34px 25px 25px
			grid-template-columns repeat(9, 1fr)
		
		@media $xs
			padding 28px 10px 25px